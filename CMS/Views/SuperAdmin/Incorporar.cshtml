@{
    ViewBag.Title = "Create";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <h4>Importar Pagina de livro</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

      <div class="form-group">    
        <label  class="control-label">Capitulo</label>
            @Html.DropDownList("cap", null, htmlAttributes: new { @class = "form-control" })       
    </div>
      <div class="form-group">    
        <label  class="control-label">Lista de videos</label>
            @Html.DropDownList("cod", null, htmlAttributes: new { @class = "form-control" })       
    </div>

    

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Incorporar" class="btn btn-default" />
        </div>
    </div>
</div>
}



@section scripts{
  
    <script type="text/javascript" src="~/js/AjaxGetGroup.js"></script>
}
